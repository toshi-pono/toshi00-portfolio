{
  "name": "toshi00-portfolio",
  "version": "1.1.0",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "pages:build": "pnpm next-on-pages",
    "preview": "pnpm pages:build && wrangler pages dev",
    "deploy": "pnpm pages:build && wrangler pages deploy",
    "prettier": "prettier --write --ignore-unknown src",
    "prettier:check": "prettier --check --ignore-unknown src",
    "test:e2e": "playwright test --grep @default",
    "test:e2e:all": "playwright test",
    "test:e2e:shot": "playwright test --grep @shot",
    "test:url": "ts-node ./tests/data/url.test.ts",
    "test": "pnpm prettier:check && pnpm lint"
  },
  "dependencies": {
    "@splidejs/react-splide": "^0.7.12",
    "next": "15.1.6",
    "normalize.css": "^8.0.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.4.0",
    "sharp": "^0.33.5"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.1",
    "@cloudflare/next-on-pages": "^1.13.7",
    "@eslint/eslintrc": "^3.2.0",
    "@playwright/test": "^1.50.1",
    "@types/node": "22.13.8",
    "@types/node-fetch": "^2.6.12",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "eslint": "^9.19.0",
    "eslint-config-next": "^15.1.6",
    "eslint-config-prettier": "^10.0.1",
    "node-fetch": "2.7.0",
    "prettier": "^3.4.2",
    "sass": "^1.83.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  },
  "pnpm": {
    "patchedDependencies": {
      "@splidejs/react-splide": "patches/@splidejs__react-splide.patch"
    },
    "overrides": {
      "tar@<6.2.1": ">=6.2.1",
      "cookie@<0.7.0": ">=0.7.0",
      "undici@>=4.5.0 <5.28.5": ">=5.28.5",
      "path-to-regexp@>=4.0.0 <6.3.0": ">=6.3.0"
    }
  },
  "packageManager": "pnpm@9.15.4"
}
