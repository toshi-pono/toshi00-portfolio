{
  "name": "toshi00-portfolio",
  "version": "1.2.2",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "pages:build": "pnpm next-on-pages",
    "preview": "pnpm pages:build && wrangler pages dev",
    "deploy": "pnpm pages:build && wrangler pages deploy",
    "prettier": "prettier --write --ignore-unknown src",
    "prettier:check": "prettier --check --ignore-unknown src",
    "test:e2e": "playwright test --grep @default",
    "test:e2e:all": "playwright test",
    "test:e2e:shot": "playwright test --grep @shot",
    "test:url": "ts-node ./tests/data/url.test.ts",
    "test": "pnpm prettier:check && pnpm lint"
  },
  "dependencies": {
    "@splidejs/react-splide": "^0.7.12",
    "next": "15.4.5",
    "normalize.css": "^8.0.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0",
    "sharp": "^0.34.2"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@cloudflare/next-on-pages": "^1.13.12",
    "@eslint/eslintrc": "^3.3.1",
    "@playwright/test": "^1.54.2",
    "@types/node": "24.1.0",
    "@types/node-fetch": "^2.6.12",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "eslint": "^9.32.0",
    "eslint-config-next": "^15.4.5",
    "eslint-config-prettier": "^10.1.8",
    "node-fetch": "2.7.0",
    "prettier": "^3.6.2",
    "sass": "^1.89.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "patchedDependencies": {
    "@splidejs/react-splide": "patches/@splidejs__react-splide.patch"
  },
  "packageManager": "pnpm@9.15.4",
  "pnpm": {
    "patchedDependencies": {
      "@splidejs/react-splide": "patches/@splidejs__react-splide.patch"
    }
  }
}
